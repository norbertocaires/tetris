#include <stdlib.h>
#include <stdio.h>
#include "lista.h"


void inicializa(listaDupla *dupla){
	dupla->primeiro = NULL;
	dupla->ultimo = NULL;
	dupla->tamanho = 0;
}

void adicionaNo(listaDupla *dupla, Dupla *no ){
	if(dupla->primeiro == NULL){
		dupla->primeiro = no;
		dupla->ultimo = no;
	}else{
		no->proximo = dupla->primeiro;
		no->anterior = dupla->ultimo;
		
		dupla->primeiro->anterior = no;
		
		dupla->ultimo->proximo = no;
		dupla->ultimo = no;
	}
	dupla->tamanho++;
}
